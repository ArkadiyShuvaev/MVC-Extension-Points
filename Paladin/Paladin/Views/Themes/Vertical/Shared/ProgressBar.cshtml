            Model.CurrentStage, (int)WorkflowStatus.AddressInfo, Model.HighestStage)
    </div>
</div>
<div class="row">
    <div class="col-md-12">
        @Html.WorkflowActionLink("VehicleInfo", "Vehicle", "Vehicle", (int)WorkflowStatus.VehicleInfo,
                Model.CurrentStage, (int)WorkflowStatus.EmploymentInfo, Model.HighestStage)
    </div>
</div>
<div class="row">
    <div class="col-md-12">
        @Html.WorkflowActionLink("ProductInfo", "Products", "Products", (int)WorkflowStatus.Products,
        Model.CurrentStage, (int)WorkflowStatus.VehicleInfo, Model.HighestStage)
    </div>
</div>
<div class="row">
    <div class="col-md-12">
        @Html.WorkflowActionLink("Final", "Home", "Final", (int)WorkflowStatus.Final,
