@model Paladin.Models.Progress
@using Paladin.Infrastructure;

<div class="row">
    <div class="col-md-12">
        @Html.WorkflowActionLink("ApplicantInfo", "Applicant", "Personal", (int)WorkflowStatus.ApplicantInfo, 
            Model.CurrentStage, (int)WorkflowStatus.Begin, Model.HighestStage)
    </div>
</div>
<div class="row">
    <div class="col-md-12">
        @Html.WorkflowActionLink("AddressInfo", "Address", "Address", (int)WorkflowStatus.AddressInfo,
        Model.CurrentStage, (int)WorkflowStatus.ApplicantInfo, Model.HighestStage)
    </div>
</div>
    <div class="row">
    <div class="col-md-12">
